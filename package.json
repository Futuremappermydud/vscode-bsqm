{
    "name": "bsqm",
    "displayName": "Beat Saber Quest Modding",
    "description": "Utilities to make modding the Oculus Quest version of Beat Saber easier",
    "version": "0.1.0",
    "publisher": "raftario",
    "engines": {
        "vscode": "^1.38.0"
    },
    "license": "SEE LICENSE IN LICENSE",
    "categories": [
        "Other"
    ],
    "keywords": [
        "beat saber",
        "modding",
        "oculus",
        "quest",
        "mod"
    ],
    "activationEvents": [
        "onCommand:bsqm.configure"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "category": "BSQM",
                "command": "bsqm.configure",
                "title": "Configure..."
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "yarn run compile",
        "test": "node ./out/test/runTest.js",
        "lint": "tslint './**/.ts'"
    },
    "dependencies": {
        "@actions/io": "^1.0.1",
        "node-fetch": "^2.6.0",
        "unzipper": "^0.10.5"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.6",
        "@types/node": "^10.12.21",
        "@types/node-fetch": "^2.5.0",
        "@types/unzipper": "^0.10.0",
        "@types/vscode": "^1.38.0",
        "glob": "^7.1.4",
        "husky": "^3.0.5",
        "lint-staged": "^9.2.5",
        "mocha": "^6.1.4",
        "tslint": "^5.12.1",
        "typescript": "^3.3.1",
        "vscode-test": "^1.2.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/raftario/vscode-bsqm.git"
    },
    "bugs": {
        "url": "https://github.com/raftario/vscode-bsqm/issues"
    },
    "homepage": "https://github.com/raftario/vscode-bsqm",
    "husky": {
        "hooks": {
            "pre-commit": "tsc && lint-staged"
        }
    },
    "lint-staged": {
        "*.ts": [
            "tslint --fix",
            "git add"
        ]
    }
}
